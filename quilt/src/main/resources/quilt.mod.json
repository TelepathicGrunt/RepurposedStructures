{
  "schema_version": 1,
  "quilt_loader": {
    "group": "com.telepathicgrunt",
    "id": "repurposed_structures",
    "version": "${version}",
    "metadata": {
      "name": "Repurposed Structures",
      "description": "Adds more variations of vanilla structures and features!",
      "contributors": {
        "TelepathicGrunt": "Owner"
      },
      "license": "LGPL-3.0-or-later",
      "icon": "assets/repurposed_structures/repurposed_structures_logo.png",
      "contact": {
        "homepage": "https://www.curseforge.com/minecraft/mc-mods/repurposed-structures",
        "issues": "https://github.com/TelepathicGrunt/RepurposedStructures/issues"
      }
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": "com.telepathicgrunt.repurposedstructures.quilt.RepurposedStructuresQuilt",
      "modmenu": "com.telepathicgrunt.repurposedstructures.configs.fabricbase.ModMenuCompat"
    },
    "depends": [
      {
        "id": "quilt_loader",
        "versions": ">=0.19.0-"
      },
      {
        "id": "quilted_fabric_api",
        "versions": ">=7.0.0-"
      },
      {
        "id": "minecraft",
        "versions": ">=1.20"
      },
      {
        "id": "midnightlib",
        "versions": ">=1.4.0"
      }
    ]
  },
  "mixin": [
    "repurposed_structures-quilt.mixins.json",
    "repurposed_structures-fabric-base.mixins.json",
    "repurposed_structures-common.mixins.json"
  ]
}
